{% extends 'base.html.twig' %}

{% block title %}Blog{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

{% block javascripts %}
    {{ parent() }}
    <script src="{{ asset('public/javascript/blog.js') }}"></script>
{% endblock %}

<form id="myForm">
  <label for="name">Nom :</label>
  <input type="text" id="name" name="name" required>

  <label for="contenu">contenu :</label>
  <textarea id="contenu" name="contenu" required></textarea>

  <label for="image">image :</label>
  <textarea id="image" name="image" required></textarea>


    <label for="date">date :</label>
  <textarea id="date" name="date" required></textarea>

    <label for="autheur">autheur :</label>
  <textarea id="autheur" name="autheur" required></textarea>


  <button type="submit">Envoyer</button>
</form>



<div id="articles"></div>


{% endblock %}
